              },
              "id": {
                "type": "string",
                "computed": true
              },
              "memory": {
                "type": "number",
                "description": "The ram of the dws node flavor in GB.",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "vcpus": {
                "type": "number",
                "description": "The vcpus of the dws node flavor.",
                "optional": true
              }
            }
          },
          "product": {
            "short": "DWS",
            "name": "Data Warehouse Service",
            "catalog": "Analytics",
            "name_cn": "数据仓库服务",
            "catalog_cn": "大数据"
          }
        }
      }
    }
  }
}
