{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_dws_flavors": {
          "block": {
            "attributes": {
              "availability_zone": {
                "type": "string",
                "description": "The availability zone name.",
                "optional": true
              },
              "datastore_type": {
                "type": "string",
                "description": "The type of datastore.",
                "optional": true
              },
              "flavors": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "availability_zones": [
                        "list",
                        "string"
                      ],
                      "datastore_type": "string",
                      "elastic_volume_specs": [
                        "list",
                        [
                          "object",
                          {
                            "max_size": "number",
                            "min_size": "number",
                            "step": "number"
                          }
                        ]
                      ],
                      "flavor_id": "string",
                      "memory": "number",
                      "size": "number",
                      "vcpus": "number",
                      "volumetype": "string"
                    }
                  ]
                ],
                "description": "The list of flavor detail.",
