              "flavor_name": {
                "type": "string",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "image_id": {
                "type": "string",
                "computed": true
              },
              "image_name": {
                "type": "string",
                "computed": true
              },
              "key_pair": {
                "type": "string",
                "computed": true
