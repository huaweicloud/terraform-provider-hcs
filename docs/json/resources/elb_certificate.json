{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_elb_certificate": {
          "block": {
            "attributes": {
              "certificate": {
                "type": "string",
                "required": true
              },
              "create_time": {
                "type": "string",
                "computed": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "domain": {
                "type": "string",
                "optional": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "expire_time": {
                "type": "string",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "optional": true
              },
              "private_key": {
                "type": "string",
                "optional": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "type": {
                "type": "string",
                "optional": true,
                "forcenew": true,
                "default": "server"
              },
              "update_time": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "ELB",
            "name": "Elastic Load Balance",
            "catalog": "Networking",
            "name_cn": "弹性负载均衡",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
