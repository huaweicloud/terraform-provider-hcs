{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_obs_bucket_policy": {
          "block": {
            "attributes": {
              "bucket": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "policy": {
                "type": "string",
                "required": true
              },
              "policy_format": {
                "type": "string",
                "optional": true,
                "default": "obs"
              },
              "region": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "OBS",
            "name": "Object Storage Service",
            "catalog": "Storage",
            "name_cn": "对象存储服务",
            "catalog_cn": "存储"
          }
        }
      }
    }
  }
}
