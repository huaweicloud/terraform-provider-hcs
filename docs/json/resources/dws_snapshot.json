{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dws_snapshot": {
          "block": {
            "attributes": {
              "cluster_id": {
                "type": "string",
                "description": "ID of the cluster for which you want to create a snapshot.",
                "required": true,
                "forcenew": true
              },
              "description": {
                "type": "string",
                "description": "Snapshot description.",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "finished_at": {
                "type": "string",
                "description": "Time when a snapshot is complete.",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "Snapshot name.",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "size": {
                "type": "number",
                "description": "Snapshot size, in GB.",
                "computed": true
              },
              "started_at": {
                "type": "string",
                "description": "Time when a snapshot starts to be created.",
                "computed": true
              },
              "status": {
                "type": "string",
                "description": "Snapshot status.",
                "computed": true
              },
              "type": {
                "type": "string",
                "description": "Snapshot type.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "DWS",
            "name": "Data Warehouse Service",
            "catalog": "Analytics",
            "name_cn": "数据仓库服务",
            "catalog_cn": "大数据"
          }
        }
      }
    }
  }
}
