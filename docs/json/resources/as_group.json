{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_as_group": {
          "block": {
            "attributes": {
              "agency_name": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "availability_zones": {
                "type": [
                  "list",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "cool_down_time": {
                "type": "number",
                "description": "The cooling duration, in seconds.",
                "optional": true,
                "default": 300
              },
              "current_instance_number": {
                "type": "number",
                "computed": true
              },
              "delete_instances": {
                "type": "string",
                "description": "Whether to delete instances when they are removed from the AS group.",
                "optional": true,
                "default": "no"
              },
              "delete_publicip": {
                "type": "bool",
                "optional": true,
                "default": false
              },
              "description": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "desire_instance_number": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "enable": {
                "type": "bool",
                "optional": true,
                "default": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "force_delete": {
                "type": "bool",
                "optional": true
              },
              "health_periodic_audit_grace_period": {
                "type": "number",
                "description": "The health check grace period for instances, in seconds.",
                "optional": true,
                "computed": true
              },
              "health_periodic_audit_method": {
                "type": "string",
                "optional": true,
                "default": "NOVA_AUDIT"
              },
              "health_periodic_audit_time": {
                "type": "number",
                "description": "The health check period for instances, in minutes.",
                "optional": true,
                "default": 5
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_terminate_policy": {
                "type": "string",
                "optional": true,
                "default": "OLD_CONFIG_OLD_INSTANCE"
              },
              "instances": {
                "type": [
                  "list",
                  "string"
                ],
                "description": "The instances id list in the as group.",
                "computed": true
              },
              "max_instance_number": {
                "type": "number",
                "optional": true,
                "default": 0
              },
              "min_instance_number": {
                "type": "number",
                "optional": true,
                "default": 0
              },
              "multi_az_scaling_policy": {
                "type": "string",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "scaling_configuration_id": {
                "type": "string",
                "description": "schema: Required",
                "required": true
              },
              "scaling_group_name": {
                "type": "string",
                "required": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true
              },
              "vpc_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              }
            },
            "block_types": {
              "lbaas_listeners": {
                "block": {
                  "attributes": {
                    "pool_id": {
                      "type": "string",
                      "required": true
                    },
                    "protocol_port": {
                      "type": "number",
                      "required": true
                    },
                    "weight": {
                      "type": "number",
                      "optional": true,
                      "default": 1
                    }
                  }
                },
                "nesting_mode": "list",
                "max_items": 6
              },
              "networks": {
                "block": {
                  "attributes": {
                    "id": {
                      "type": "string",
                      "required": true
                    },
                    "ipv6_bandwidth_id": {
                      "type": "string",
                      "optional": true
                    },
                    "ipv6_enable": {
                      "type": "bool",
                      "optional": true
                    },
                    "source_dest_check": {
                      "type": "bool",
                      "optional": true,
                      "default": true
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 5
              },
              "security_groups": {
                "block": {
                  "attributes": {
                    "id": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list"
              }
            }
          },
          "product": {
            "short": "AS",
            "name": "Auto Scaling",
            "catalog": "Compute",
            "name_cn": "弹性伸缩",
            "catalog_cn": "计算"
          }
        }
      }
    }
  }
}
