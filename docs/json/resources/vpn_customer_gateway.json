{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_vpn_customer_gateway": {
          "block": {
            "attributes": {
              "asn": {
                "type": "number",
                "description": "The BGP ASN number of the customer gateway, only required when the route_mode is bgp, the default value is 65000.",
                "optional": true,
                "forcenew": true,
                "default": 65000
              },
              "created_at": {
                "type": "string",
                "description": "The create time.",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "ip": {
                "type": "string",
                "description": "The IP address of the customer gateway.",
                "required": true,
                "forcenew": true
              },
              "name": {
                "type": "string",
                "description": "The customer gateway name.",
                "required": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "route_mode": {
                "type": "string",
                "description": "The route mode of the customer gateway.",
                "optional": true,
                "forcenew": true,
                "default": "bgp"
              },
              "updated_at": {
                "type": "string",
                "description": "The update time.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "VPN",
            "name": "Virtual Private Network",
            "catalog": "Networking",
            "name_cn": "虚拟专用网络",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
