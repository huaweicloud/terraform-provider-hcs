{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_vpc_address_group": {
          "block": {
            "attributes": {
              "addresses": {
                "type": [
                  "set",
                  "string"
                ],
                "required": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "ip_version": {
                "type": "number",
                "optional": true,
                "forcenew": true,
                "default": 4
              },
              "name": {
                "type": "string",
                "required": true
              },
              "region": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "VPC",
            "name": "Virtual Private Cloud",
            "catalog": "Networking",
            "name_cn": "虚拟私有云",
            "catalog_cn": "网络"
          }
        }
      }
    }
  }
}
