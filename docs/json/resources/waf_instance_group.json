{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_waf_instance_group": {
          "block": {
            "attributes": {
              "body_limit": {
                "type": "number",
                "computed": true
              },
              "connection_timeout": {
                "type": "number",
                "computed": true
              },
              "description": {
                "type": "string",
                "optional": true
              },
              "header_limit": {
                "type": "number",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "load_balancers": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "name": {
                "type": "string",
                "required": true
              },
              "read_timeout": {
                "type": "number",
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "vpc_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "write_timeout": {
                "type": "number",
                "computed": true
              }
            }
          },
          "product": {
            "short": "WAF",
            "name": "Web Application Firewall",
            "catalog": "Security & Compliance",
            "name_cn": "Web应用防火墙",
            "catalog_cn": "安全与合规"
          }
        }
      }
    }
  }
}
