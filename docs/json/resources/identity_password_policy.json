{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_identity_password_policy": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "maximum_consecutive_identical_chars": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "maximum_password_length": {
                "type": "number",
                "computed": true
              },
              "minimum_password_age": {
                "type": "number",
                "optional": true,
                "computed": true
              },
              "minimum_password_length": {
                "type": "number",
                "optional": true,
                "default": 8
              },
              "number_of_recent_passwords_disallowed": {
                "type": "number",
                "optional": true,
                "default": 1
              },
              "password_char_combination": {
                "type": "number",
                "optional": true,
                "default": 2
              },
              "password_not_username_or_invert": {
                "type": "bool",
                "optional": true,
                "default": true
              },
              "password_validity_period": {
                "type": "number",
                "optional": true,
                "computed": true
              }
            }
          },
          "product": {
            "short": "IAM",
            "name": "Identity and Access Management",
            "catalog": "Management & Governance",
            "name_cn": "统一身份认证服务",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
