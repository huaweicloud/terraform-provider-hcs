{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_apig_environment": {
          "block": {
            "attributes": {
              "created_at": {
                "type": "string",
                "description": "The time when the environment was created.",
                "computed": true
              },
              "description": {
                "type": "string",
                "description": "The environment description.",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the dedicated instance to which the environment belongs.",
                "required": true,
                "forcenew": true
              },
              "name": {
                "type": "string",
                "description": "The environment name.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the dedicated instance is located.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "APIG",
            "name": "API Gateway",
            "catalog": "Middleware",
            "name_cn": "API网关",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
