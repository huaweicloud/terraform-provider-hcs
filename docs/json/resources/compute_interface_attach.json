{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_compute_interface_attach": {
          "block": {
            "attributes": {
              "fixed_ip": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "mac": {
                "type": "string",
                "computed": true
              },
              "network_id": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "port_id": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "security_group_ids": {
                "type": [
                  "list",
                  "string"
                ],
                "optional": true,
                "computed": true
              },
              "source_dest_check": {
                "type": "bool",
                "optional": true,
                "default": true
              }
            }
          },
          "product": {
            "short": "ECS",
            "name": "Elastic Cloud Server",
            "catalog": "Compute",
            "name_cn": "弹性云服务器",
            "catalog_cn": "计算"
          }
        }
      }
    }
  }
}
