                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "description": "The charging period unit.",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              }
            }
          },
          "product": {
            "short": "DSC",
            "name": "Data Security Center",
            "catalog": "Security & Compliance",
