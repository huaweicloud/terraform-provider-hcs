{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_cci_pvc": {
          "block": {
            "attributes": {
              "access_modes": {
                "type": [
                  "list",
                  "string"
                ],
                "computed": true
              },
              "creation_timestamp": {
                "type": "string",
                "computed": true
              },
              "device_mount_path": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "enable": {
                "type": "bool",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "namespace": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "volume_id": {
                "type": "string",
                "required": true,
                "forcenew": true
              },
              "volume_type": {
                "type": "string",
                "optional": true,
                "forcenew": true,
                "default": "sas"
              }
            }
          },
          "product": {
            "short": "CCI",
            "name": "Cloud Container Instance",
            "catalog": "Containers",
            "name_cn": "云容器实例",
            "catalog_cn": "容器"
          }
        }
      }
    }
  }
}
